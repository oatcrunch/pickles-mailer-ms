{
    "name": "pickle-mailer-ms",
    "version": "0.1.0",
    "bin": {
        "pickle-mailer-ms": "bin/pickle-mailer-ms.js"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest --detectOpenHandles",
        "synth": "cdk synth",
        "deploy": "cdk deploy",
        "destroy": "cdk destroy",
        "scan": "npx prettier --check \"bin/**/*.ts\" \"lib/**/*.ts\" \"test/**/*.ts\" \"modules/**/*.ts\"",
        "fix": "npx prettier --write .",
        "build-start": "npm run build && npm run start",
        "postinstall": "cd modules/mailer-service && npm i"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.111",
        "@types/dotenv": "^8.2.0",
        "@types/jest": "^29.4.0",
        "@types/mime-types": "^2.1.1",
        "@types/node": "18.11.18",
        "@types/uuid": "^9.0.1",
        "aws-cdk": "2.64.0",
        "jest": "^29.4.1",
        "prettier": "^2.8.4",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "~4.9.5"
    },
    "author": "Melvin Liew",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.292.0",
        "@aws-sdk/client-eventbridge": "^3.290.0",
        "@aws-sdk/client-s3": "^3.294.0",
        "@aws-sdk/util-dynamodb": "^3.292.0",
        "aws-cdk-lib": "2.64.0",
        "aws-lambda": "^1.0.7",
        "constructs": "^10.0.0",
        "dotenv": "^16.0.3",
        "mime-types": "^2.1.35",
        "source-map-support": "^0.5.21"
    }
